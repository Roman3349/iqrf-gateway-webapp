{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/cloudInteliGlue.json",
	"type": "object",
	"title": "Inteliments InteliGlue connection",
	"additionalProperties": false,
	"required": [
		"rootTopic",
		"assignedPort",
		"clientId",
		"password"
	],
	"properties": {
		"rootTopic": {
			"$id": "#/properties/rootTopic",
			"type": "string",
			"title": "Root topic",
			"example": "root"
		},
		"assignedPort": {
			"$id": "#/properties/assignedPort",
			"type": "integer",
			"title": "Assigned port",
			"example": 1234
		},
		"clientId": {
			"$id": "#/properties/clientId",
			"type": "string",
			"title": "Client ID",
			"example": "client1234"
		},
		"password": {
			"$id": "#/properties/password",
			"type": "string",
			"title": "Password",
			"example": "pass1234"
		}
	}
}
